<link rel="stylesheet" type="text/css" href="style.css" media="screen" title="Normal"/>
<script type="text/javascript"> 
filename = new String(document.location);
filename = filename.substring(1 + filename.lastIndexOf('/'), filename.length);
var current      = filename.substring(0, filename.lastIndexOf('_'));
var current_lang = "";
var _lang = filename.substring(1 + filename.lastIndexOf('_'), filename.lastIndexOf('.'));
var current_ens  = "liste";
if (current.length == 0) {
	current      = "index";
}
if (_lang.length == 0) {
	_lang = "fr";
}

function show(id) {
	x = document.getElementById(current + "_" + current_lang);
	if (null == x) {
		x = document.getElementById(current);
	}
	if (null != x) {
		x.style.display = 'none';
	}
	
	y = document.getElementById(id + "_" + current_lang);
	if (null == y) {
		y = document.getElementById(id);
	}
	if (null != y) {
		y.style.display = 'block';
	}
	
	current = id;
}
function show_ens(id) {
	document.getElementById(current_ens + "ens").style.backgroundColor = 'midnightblue';
	document.getElementById(current_ens).style.display                 = 'none';
	document.getElementById(id + "ens").style.backgroundColor  = 'mediumblue';
	document.getElementById(id).style.display                  = 'block';
	current_ens = id;
}
function change_language(l) {
	if (l != current_lang) {

		x = document.getElementById(current + "_" + current_lang);
		if (x != null) {
			x.style.display = 'none';
		}

		y = document.getElementById(current + "_" + l);
		if (y != null) {
			y.style.display      = 'block';
		}

		current_lang = l;
		maj_enseignements();
		maj_menu();
		maj_publis();
		maj_titre();
	}
}
function maj_menu() {
	if (current_lang == "en") {
		document.getElementById("menu_accueil").innerHTML       = "Home";
		document.getElementById("menu_liens").innerHTML         = "Useful pointers";
		document.getElementById("menu_enseignements").innerHTML = "Courses";
	}
	else {
		document.getElementById("menu_accueil").innerHTML       = "Accueil";
		document.getElementById("menu_liens").innerHTML         = "Liens utiles";
		document.getElementById("menu_enseignements").innerHTML = "Enseignements";
	}
}
function maj_publis() {
	if (current_lang == "en") {
		document.getElementById("publi_jour").innerHTML = "International or national journals";
		document.getElementById("publi_intc").innerHTML = "International conferences";
		document.getElementById("publi_autc").innerHTML = "Workshops and other conferences with evaluation committee";
	}
	else {
		document.getElementById("publi_jour").innerHTML = "Revues nationales ou internationales";
		document.getElementById("publi_intc").innerHTML = "Conférences internationales";
		document.getElementById("publi_autc").innerHTML = "Ateliers et autres conférences avec comité de lecture";
	}
}
function maj_titre() {
	if (current_lang == "en") {
		document.title = "Sébastien Guérif - Machine Learning - Data Mining";
	}
	else {
		document.title = "Sébastien Guérif - Apprentissage artificiel - Fouille de données";
	}
}
function maj_enseignements() {
	if (current_lang == "en") {
		document.getElementById("ensh1").innerHTML      = "Courses (only in french)";
		document.getElementById("listeens").innerHTML   = "List";
		document.getElementById("licenceens").innerHTML = "Bachelor";
	}
	else {
		document.getElementById("ensh1").innerHTML      = "Enseignements";
		document.getElementById("listeens").innerHTML   = "Liste";
		document.getElementById("licenceens").innerHTML = "Licence";
	}
}
</script>

</head>

