\begin{frame}
\frametitle{Complex Systems Concept}
\begin{block}{Interactive entities}
\begin{tabular}{cc}
\begin{minipage}{4cm}
\begin{itemize}
\item Systems involve a number of interacting entities:
\item Each entity has local relationships;
\end{itemize}
\end{minipage} &
\begin{minipage}{6cm}
\begin{center}
\includegraphics[scale=1.7]{Complex-adaptive-system-1.jpg}
\end{center}
\end{minipage}
\end{tabular}
\end{block}
\end{frame}

\begin{frame}
\frametitle{Complex Systems Concept}
\begin{block}{Emergent System Properties}
\begin{tabular}{cc}
\begin{minipage}{5cm}
\begin{itemize}
\item System properties emerge from its own entities interactions;
\item Emergent properties  are not expressed by the behavior of the constitutive entities, themselves.
\end{itemize}
\begin{center}
\textbf{"The system is more than the sum of its parts"}
\end{center}
\end{minipage} &
\begin{minipage}{5.5cm}
\begin{center}
\includegraphics[scale=1.5]{Complex-adaptive-system-2.jpg}
\end{center}
\end{minipage}
\end{tabular}
\end{block}
\end{frame}

