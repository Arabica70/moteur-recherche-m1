	function toggle(id)
	{
		var item = document.getElementById(id);
		if (item.className == 'hidden') item.className = 'visible';
		else item.className = 'hidden';
	}
	function hide(id)
	{
		var item = document.getElementById(id);
		item.className = 'hidden';
	}
