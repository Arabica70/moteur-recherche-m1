		$(".longTextFieldType#containerField51").html("<textarea class=\"fieldinput\" name=\"field51\" title=\"\">"+""+"</textarea>");
		autosize($('.longTextFieldType textarea'));

		$(".enumFieldType#containerField51").each(function() {
			var vals = "min=1,max=100000".split(",");
			var opts = "";
			var nbvals = vals.length;
			for(var i = 0; i < nbvals; i++)
			{
				var val = vals[i];
				opts += "<option value=\""+val+"\">"+val+"</option>\n";
			}
			$(this).html("<select class=\"fieldinput\" name=\"field51\" title=\"\">"+opts+"</select>");
			}
		);
		$("#containerFieldInput51").spinner();
		</script>
	</td>
</tr>
